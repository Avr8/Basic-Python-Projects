      "source": [
        "def initialize_with_zeros(dim):\r\n",
        "  W = np.zeros(shape=(dim,1))\r\n",
        "  b = 0\r\n",
        "\r\n",
        "  assert(W.shape == (dim, 1))\r\n",
        "  assert(isinstance(b, float) or isinstance(b, int))\r\n",
        "    \r\n",
        "  return W, b"
      ],
      "execution_count": 50,
      "outputs": []
    },
